{
  "root": true,
  "extends": "eslint:recommended",
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaVersion": 2019,
    "sourceType": "module",
    "experimentalDecorators": true,
    "ecmaFeatures": {
      "legacyDecorators": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}
